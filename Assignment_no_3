#include <stdio.h>
int main() {
int n, i, j, temp;
printf("Enter the size of the array: ");
scanf("%d", &n);
int a[n];  
    printf("Enter the array elements:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }
for (i = 0; i < n - 1; i++) {
      for (j = i+1; j < n; j++) {
          if (a[j]<a[i]) {
                temp = a[i];
                a[i] = a[j];
                a[j] = temp;
            }
        }
    }
    printf("The sorted array is: ");
    for (i = 0; i < n; i++) {
        printf("%d\t", a[i]);
    }
    printf("\n");
return 0;
}
