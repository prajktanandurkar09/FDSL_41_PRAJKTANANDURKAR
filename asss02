#include<stdio.h>



int main(){
int n,a[n],i,j,temp;
printf("enter the size of array:- ");
scanf("%d",&n);

printf("enter the array elements:-");
 for(i=0;i<n;i++){
 scanf("%d",&a[i]);
 }
 for(j=0;j<n;i++){
  	for(j=0;j<n-i-1;j++){
  	if(a[j]>a[j+1]){
  	temp=a[j];
  	a[j]= a[j+1];
  	a[j+1]=temp;
  	}
       }
 }
 printf("the sorted arrey are :-");
 for(i=0;i<n;i++){
 printf("%d\t",a[i]);
 }
 return 0;
 }
  	
