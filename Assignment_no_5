#include<stdio.h>
int main(){
int i ,n,key,found=0;
printf("enter the size of array:");
scanf("%d",&n);
int a[n];
printf("enter the array elemnts:");
for(i=1;i<=n;i++){
scanf("%d",&a[i]);
  }
  printf("enter the element you want to search:");
  scanf("%d",&key);
  
  for(i=0;i<n;i++){
    if(key == a[i]){
      
     
      found=1;
      break;
      }
      }
      if(found==1){
       printf("the element is present at %d th position",i);
       }
    else{
      printf("the element is not present in array");
      }
      
      return 0;
      }
      
  
