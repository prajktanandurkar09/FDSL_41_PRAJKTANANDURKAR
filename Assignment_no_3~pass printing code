no#include <stdio.h>
int main() {
int n, i, j,k, temp;
printf("Enter the size of the array: ");
scanf("%d", &n);
int a[n];  
    printf("Enter the array elements:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }
for (i = 0; i < n - 1; i++) {  
printf("\npass %d is ",i+1);
for(k=0;k<n;k++){
       
       printf("\n at %d position :",k+1);
        printf("%d",a[k]);
        }
      for (j = i+1; j < n; j++) {
          if (a[j]<a[i]) {
                a[i]=a[i]+a[j];
                a[j]=a[i]-a[j];
                a[i]=a[i]-a[j];
            }
            
        }
        
    }
    printf("\nThe sorted array is: ");
    for (i = 0; i < n; i++) {
        printf("%d\t", a[i]);
    }
    printf("\n");
return 0;
}



#include <stdio.h>
#include <string.h>

// Define a structure
struct Person {
    char name[50];
    int age;
};

// Function to swap two structure elements
void swap(struct Person *a, struct Person *b) {
    struct Person temp = *a;
    *a = *b;
    *b = temp;
}

// Bubble Sort function to sort by age
void bubbleSort(struct Person arr[], int n) {
    int i, j;
    for (i = 0; i < n - 1; i++) {
        for (j = 0; j < n - i - 1; j++) {
            if (arr[j].age > arr[j + 1].age) {  // Sorting by age
                swap(&arr[j], &arr[j + 1]);
            }
        }
    }
}

// Function to print the array of structures
void printArray(struct Person arr[], int n) {
    for (int i = 0; i < n; i++) {
        printf("Name: %s, Age: %d\n", arr[i].name, arr[i].age);
    }
}

int main() {
    struct Person people[] = {
        {"Alice", 25},
        {"Bob", 30},
        {"Charlie", 22},
        {"David", 28}
    };
    
    int n = sizeof(people) / sizeof(people[0]);

    printf("Before sorting:\n");
    printArray(people, n);

    // Sort the structure array
    bubbleSort(people, n);

    printf("\nAfter sorting by age:\n");
    printArray(people, n);

    return 0;
}
