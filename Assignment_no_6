#include<stdio.h>
int main(){
int i, key ,low=0,n;
printf("enetr the size of array :");
scanf("%d",&n);
int a[n],high=n-1;
printf("enetr %d array elements:",n);
for(i=0;i<n;i++){
scanf("%d",&a[i]);
}
 printf("enter the no. you want to search:");
 scanf("%d", &key);
      while(high >=low){
          int mid= (low+high)/2;
             if(key==a[mid]){
             printf("element found at %d  index:",mid);
             
                return 0;
                }
             else if(key>a[mid]){
               low=mid+1;
               }
             else{
               high=mid-1;
               }
          }
     if(high<low){
     printf("element not found:");
     }
  return 0;
  }
